{
  "name": "node",
  "version": "1.0.0",
  "type": "module",
  "description": "Learn Node JS and Create Rest API",
  "main": "index.js",
  "scripts": {
    "start": "npx tsc -w",
    "dev": "npx nodemon",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,md}\"",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check \"src/**/*.{ts,tsx,js,json,md}\"",
    "check-lint": "eslint . --ext ts --ext tsx --ext js"
  },
  "author": "Reza Aditama",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/express": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "3.5.3",
    "pretty-quick": "^4.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.1"
  },
  "dependencies": {
    "express": "^5.1.0",
    "nodemon": "^3.1.9"
  }
}
